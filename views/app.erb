<script type="text/template" id="venue-listitem-template">
   <div class="result_title">{{= venue.get('name') }}</div>
   <div class="result_image"><img src="{{= venue.get('photo_url').replace(/https?\:/, '') }}" alt="{{= venue.get('name') }}"></div>
   <div class="result_rating"><img src="{{= venue.get('rating_img_url_small').replace(/https?\:/, '') }}" alt="{{= venue.get('avg_rating') }}"></div>
   <div class="result_rating_count">({{= venue.get('review_count') }})</div>
   <div class="description_container">
      <div class="result_address">{{= venue.get('address') }}</div>
      <div class="result_phone_container">
         <div class="result_phone">{{= venue.get('formattedPhoneNumber') }}</div>
      </div>
      <div class="result_description">
         <p class="description_text">
         {{= venue.get('reviewText') }}
          <a class="result_readmore" href="{{= venue.get('url') }}" target="_blank">Read More&raquo;</a>
         </p>
      </div>
   </div>
   <div class="result_button"><a class="panel" href="#"><img src="/images/venuebutton.png" alt="Choose this Venue"></a>
   </div>
</script>

